    </dependency>  
    <dependency> 
      <groupId>org.springframework</groupId>  
      <artifactId>spring-websocket</artifactId>  
      <version>4.0.1.RELEASE</version> 
    </dependency>  
    <dependency> 
      <groupId>javax.servlet.jsp.jstl</groupId>  
      <artifactId>javax.servlet.jsp.jstl-api</artifactId>  
