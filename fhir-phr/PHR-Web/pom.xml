    </dependency>  
    <dependency> 
      <groupId>org.glassfish.web</groupId>  
      <artifactId>javax.servlet.jsp.jstl</artifactId>  
      <version>1.2.2</version> 
    </dependency>  
    <dependency> 
      <groupId>${project.groupId}</groupId>  
      <artifactId>PHR-Lib</artifactId>  
      <version>${project.version}</version> 
