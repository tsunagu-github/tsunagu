{
    "name": "helloworld",
    "displayName": "HelloCordova",
    "version": "1.0.0",
    "description": "A sample Apache Cordova application that responds to the deviceready event.",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "ecosystem:cordova"
    ],
    "author": "Apache Cordova Team",
    "license": "Apache-2.0",
    "dependencies": {
        "cordova-android": "^9.0.0",
        "cordova-plugin-androidx-adapter": "git+https://github.com/dpa99c/cordova-plugin-androidx-adapter.git",
        "cordova-plugin-customurlscheme": "^5.0.2",
        "cordova-plugin-fcm": "git+https://github.com/fechanique/cordova-plugin-fcm.git",
        "cordova-plugin-file": "^6.0.2",
        "cordova-plugin-inappbrowser": "git+https://github.com/apache/cordova-plugin-inappbrowser.git",
        "cordova-plugin-network-information": "git+https://github.com/apache/cordova-plugin-network-information.git",
        "cordova-plugin-whitelist": "git+https://github.com/apache/cordova-plugin-whitelist.git",
        "cordova.plugins.diagnostic": "^6.1.1",
        "mkdirp": "^1.0.4",
        "phonegap-plugin-barcodescanner": "git+https://github.com/phonegap/phonegap-plugin-barcodescanner.git"
    },
    "cordova": {
        "platforms": [
            "android"
        ],
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-network-information": {},
            "cordova-plugin-inappbrowser": {},
            "cordova-plugin-fcm": {},
            "cordova-plugin-androidx-adapter": {},
            "cordova-plugin-customurlscheme": {
                "URL_SCHEME": "net.allm.MySOS",
                "ANDROID_SCHEME": " ",
                "ANDROID_HOST": " ",
                "ANDROID_PATHPREFIX": "/"
            },
            "phonegap-plugin-barcodescanner": {
                "ANDROID_SUPPORT_V4_VERSION": "27.+"
            },
            "cordova-plugin-file": {},
            "cordova.plugins.diagnostic": {
                "ANDROIDX_VERSION": "1.0.0",
                "ANDROIDX_APPCOMPAT_VERSION": "1.3.1"
            }
        }
    }
}